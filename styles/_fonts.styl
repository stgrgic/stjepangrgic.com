/* work-sans-regular - latin */
@font-face {
  font-family: 'Work Sans';
  font-style: normal;
  font-weight: 400;
  src: url('../static/fonts/work-sans-v5-latin-regular.eot'); /* IE9 Compat Modes */
  src: local('Work Sans'), local('WorkSans-Regular'),
       url('../static/fonts/work-sans-v5-latin-regular.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
       url('../static/fonts/work-sans-v5-latin-regular.woff2') format('woff2'), /* Super Modern Browsers */
       url('../static/fonts/work-sans-v5-latin-regular.woff') format('woff'), /* Modern Browsers */
       url('../static/fonts/work-sans-v5-latin-regular.ttf') format('truetype'), /* Safari, Android, iOS */
       url('../static/fonts/work-sans-v5-latin-regular.svg#WorkSans') format('svg'); /* Legacy iOS */
}
